{
    "name": "filippo-toso/laravel-supervisor",
    "description": "A simple supervisor tool that allows to run long lived Artisan commands (i.e. the queue) from the crontab",
    "license": "MIT",
    "authors": [{
        "name": "Filippo Toso",
        "email": "filippo.toso@creativepark.it"
    }],
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "FilippoToso\\LaravelSupervisor\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilippoToso\\LaravelSupervisor\\SupervisorServiceProvider"
            ]
        }
    }
}