{
    "name": "filippo-toso/laravel-supervisor",
    "description": "A simple supervisor tool that allows to run the Laravel Queue from the crontab",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Toso",
            "email": "filippo.toso@creativepark.it"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "illuminate/support": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "FilippoToso\\LaravelSupervisor\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FilippoToso\\LaravelSupervisor\\SupervisorServiceProvider"
            ]
        }
    }
}
